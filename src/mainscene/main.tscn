resource_name = "spin"
script/source = "extends TextureRect

@onready var rewind: Rewind = $\"../../../Rewind\"

@onready var speed = randf()+1
@onready var rewind_clock: Control = $\"..\"


func _process(delta: float) -> void:
	rewind_clock.visible = rewind.rewinding
	rotation -= delta * 10.0 * speed
	
"

